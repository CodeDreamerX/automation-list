{
  "files.associations": {
    "*.svg": "xml"
  },
  "diffEditor.ignoreTrimWhitespace": true,
  "diffEditor.renderSideBySide": false,
  "diffEditor.enableSyntaxHighlighting": false,
  "files.exclude": {
    "**/src/icons/categories/**": false
  },
  "search.exclude": {
    "**/src/icons/categories/**": true
  },
  "[svg]": {
    "editor.diffEditor.ignoreTrimWhitespace": true,
    "editor.formatOnSave": false
  },
  "files.watcherExclude": {
    "**/src/icons/categories/**": true,
    "**/public/icons/categories/**": true
  }
}

